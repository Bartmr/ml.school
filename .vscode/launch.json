{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "program": "${file}",
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "justMyCode": true
    }
  ]
}
